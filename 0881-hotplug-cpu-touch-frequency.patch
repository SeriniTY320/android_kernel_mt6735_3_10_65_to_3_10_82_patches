Subject: [PATCH 0881/1168] hotplug cpu touch frequency

Former-commit-id: 7b02d96f99ce437e056804bf3940d54b9df5121f
---
 drivers/cpufreq/cpufreq_hotplug.c | 3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/drivers/cpufreq/cpufreq_hotplug.c b/drivers/cpufreq/cpufreq_hotplug.c
index 942a28d..a70d822 100755
--- a/drivers/cpufreq/cpufreq_hotplug.c
+++ b/drivers/cpufreq/cpufreq_hotplug.c
@@ -1586,7 +1586,8 @@ static int touch_freq_up_task(void *data)
 
 	while (1) {
 		policy = cpufreq_cpu_get(0);
-		dbs_freq_increase(policy, policy->max);
+		if (policy->cur < 820000)
+			dbs_freq_increase(policy, 820000);
 		cpufreq_cpu_put(policy);
 		/* mt_cpufreq_set_ramp_down_count_const(0, 100); */
 		pr_debug("@%s():%d\n", __func__, __LINE__);
